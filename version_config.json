type CloudBaseRunVersionConfig struct {
	ContainerPort int32               `json:"ContainerPort"`
	ContainerSpec ContainerSpecConfig `json:"ContainerSpec"`
	Capacity      CapacityConfig      `json:"Capacity"`
	Log           LogConfig           `json:"Log"`
	EnvParams     string              `json:"EnvParams"`
}

type ContainerSpecConfig struct {
	Cpu float32 `json:"Cpu"`
	Mem float32 `json:"Mem"`
}

//CapacityConfig strcut implement
type CapacityConfig struct {
	MinNum          int32  `json:"MinNum"`
	MaxNum          int32  `json:"MaxNum"`
	PolicyType      string `json:"PolicyType"`
	PolicyThreshold int32  `json:"PolicyThreshold"`
}

//LogConfig strcut implement
type LogConfig struct {
	CustomLogs string `json:"CustomLogs"`
}